<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">

<dwr>

  <allow>

		<create javascript="NewsAction" creator="spring">
			<param name="beanName" value="newsAction"></param>
		</create>
		
		<create javascript="ColumnAction" creator="spring">
			<param name="beanName" value="columnAction"></param>
		</create>
		
		<create javascript="WebsiteAction" creator="spring">
			<param name="beanName" value="websiteAction"></param>
		</create>
		
		<create javascript="NoticeAction" creator="spring">
			<param name="beanName" value="noticeAction"></param>
		</create>

		<create javascript="CarAction" creator="spring">
			<param name="beanName" value="carAction"></param>
		</create>
		
		<create javascript="CartypeAction" creator="spring">
			<param name="beanName" value="cartypeAction"></param>
		</create>
		<create javascript="SysUserSvc" creator="spring">
			<param name="beanName" value="sysUserSvc"></param>
		</create>
		
		 
		<create javascript="SysMenuSvc" creator="spring">
			<param name="beanName" value="sysMenuSvc"></param>
		</create>
		
		<create javascript="SysButtonSvc" creator="spring">
			<param name="beanName" value="sysButtonSvc"></param>
		</create>
		
		<create javascript="SysRoleSvc" creator="spring">
			<param name="beanName" value="sysRoleSvc"></param>
		</create>
		
		<create javascript="SysPermissionSvc" creator="spring">
			<param name="beanName" value="sysPermissionSvc"></param>
		</create>
	
	<create javascript="SysHolidaySvc" creator="spring">
			<param name="beanName" value="sysHolidaySvc"></param>
		</create>
	
		

		
		<!-- 定义允许dwr将User实例转换成JavaScript对象 -->
		<convert match="com.unis.app.login.model.User" converter="bean"/>
		<convert match="com.unis.app.news.model.News" converter="bean"/>
		<convert match="com.unis.app.column.model.Column" converter="bean"/>
		<convert match="com.unis.app.website.model.Website" converter="bean"/>
		<convert match="com.unis.core.commons.Combox" converter="bean"/>
		<convert match="com.unis.app.notice.model.Notice" converter="bean"/>
		<convert match="com.unis.app.car.model.Car" converter="bean"/>
	
  </allow>

</dwr>
